<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"></script>

<script>
  // Sahifa yuklanganda faqat Home ko'rinmasin, bosh sahifa uchun o'zgartirish kerak bo'lsa shu yerda qo'shish mumkin
  // Hozirda hech qanday boâ€˜lim koâ€˜rinmaydi, faqat alohida boâ€˜lim koâ€˜rsatish funksiyasi ishlaydi

  function showSection(sectionId) {
    // Barcha bo'limlarni yashirish
    const sections = document.querySelectorAll('section');
    sections.forEach(sec => {
      sec.classList.add('d-none');
      sec.classList.remove('visible');
    });

    // Tanlangan boâ€˜limni koâ€˜rsatish
    const section = document.getElementById(sectionId);
    if (section) {
      section.classList.remove('d-none');
      // Agar fade-in klassi boâ€˜lsa, visible klassini qoâ€˜shamiz
      if (section.classList.contains('fade-in')) {
        setTimeout(() => section.classList.add('visible'), 10);
      }
    }
  }
</script>

